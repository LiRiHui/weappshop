
.vip{
  width: 100%;
  height: 100rpx;
  font-size: 25rpx;
  border-width: 2rpx;
  border-top-style: solid;
  border-top-color: #585755;
  border-bottom-style: solid;
  border-bottom-color: #3a3835;
}

.vip image{
  float:left; 
  margin-top:2rpx;
  margin-bottom:2rpx;
  margin-left:10rpx;
  margin-right:10rpx;  
  width: 125rpx;
  height: 90rpx;
}

.vip-cell-first{
  border-top:none;
}

.vip-cell-last{
  border-bottom: none;
}

.vip-cell-text{
  padding-left: 5rpx;
  width: 480rpx;
  float:left;
  margin-top:10rpx;
  margin-left:20rpx; 
}

.vip-text-line{
  height: 40rpx;
  line-height: 40rpx;
}