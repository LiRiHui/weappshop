
.room{
  float: left;
  display: table;
  width: 100%;
  height: 110rpx;
  font-size: 25rpx;
  border-width: 2rpx;
  border-bottom-style: dashed;
  border-bottom-color: #3a3835;
}

.room image{
  float:left; 
  margin-top:10rpx;
  margin-bottom:10rpx; 
  margin-left:10rpx;
  margin-right:10rpx;  
  width: 60rpx;
  height: 80rpx;
}

.room-claw{
  height: 40rpx;
}

.room-cell-first{
  border-top:none;
}

.room-cell-last{
  border-bottom: none;
}

.room-cell-text{
  display: table-cell;
  vertical-align: middle;
  padding-left: 5rpx;
  width: 480rpx;
  float:left;
  /* margin-top:10rpx; */
  margin-left:20rpx; 
}

.room-text-line{
  height: 40rpx;
  line-height: 40rpx;
  display:inline-block;
}