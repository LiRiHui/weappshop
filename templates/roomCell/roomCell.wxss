
.room{
  width: 100%;
  height: 180rpx;
  font-size: 25rpx;
  border-width: 2rpx;
  border-bottom-style: dashed;
  border-bottom-color: #3a3835;
}

.room image{
  float:left; 
  margin-top:15rpx;
  margin-bottom:15rpx;
  margin-left:10rpx;
  margin-right:10rpx;  
  width: 150rpx;
  height: 150rpx;
}

.room-cell-first{
  border-top:none;
}

.room-cell-last{
  border-bottom: none;
}

.room-cell-text{
  padding-left: 5rpx;
  width: 480rpx;
  float:left;
  margin-top:10rpx;
  margin-left:20rpx; 
}

.room-text-line{
  height: 40rpx;
  line-height: 40rpx;
}